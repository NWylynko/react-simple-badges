{
  "name": "react-simple-badges",
  "type": "module",
  "version": "3.1.0-beta.8",
  "description": "React badges for your Profile and Projects.",
  "author": "NWylynko",
  "license": "MIT",
  "repository": "NWylynko/react-simple-badges",
  "exports": {
    "./*": {
      "types": "./dist/esm/badges/*.d.ts",
      "node": {
        "require": "./dist/cjs/badges/*.js",
        "import": "./dist/es2015/badges/*.js"
      },
      "default": "./dist/esm/badges/*.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --declaration --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "dev": "tsc -w",
    "release": "bumpp --commit --push --tag",
    "fetchColours": "cd fetchColours && yarn install && yarn build && yarn start",
    "generateBadges": "cd generateBadges && yarn install && yarn build && yarn start"
  },
  "peerDependencies": {
    "react": ">16.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "bumpp": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.8.4",
    "npm-run-all": "^4.1.5"
  },
  "files": [
    "dist",
    "empty.d.ts"
  ],
  "dependencies": {},
  "types": "./empty.d.ts"
}
